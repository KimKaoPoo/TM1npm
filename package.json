{
  "name": "tm1npm",
  "version": "1.0.0",
  "description": "A Node.js module for TM1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:ci": "jest --config jest.ci.config.js",
    "test:coverage:ci": "jest --config jest.ci.config.js --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "VERBOSE_TESTS=true jest --verbose",
    "test:connection": "ts-node tests/connection/test-connection.ts",
    "test:simple": "ts-node tests/connection/simple-connection-test.ts",
    "test:minimal": "ts-node tests/connection/minimal-connection-test.ts",
    "test:working": "ts-node tests/integration/working-test.ts",
    "test:security": "ts-node tests/security/security-test.ts",
    "test:performance": "ts-node tests/performance/performance-test.ts",
    "test:edge-cases": "ts-node tests/edge-cases/edge-cases-test.ts",
    "test:process-service": "jest src/tests/processService.test.ts",
    "test:dimension-service": "jest src/tests/dimensionService.test.ts",
    "test:cube-service": "jest src/tests/cubeService.test.ts",
    "test:rest-service": "jest src/tests/restService.test.ts",
    "test:subset-service": "jest src/tests/subsetService.test.ts",
    "test:view-service": "jest src/tests/viewService.test.ts",
    "test:element-service": "jest src/tests/elementService.test.ts",
    "test:hierarchy-service": "jest src/tests/hierarchyService.test.ts",
    "test:cell-service": "jest src/tests/cellService.test.ts",
    "test:comprehensive": "jest src/tests/comprehensive.service.test.ts",
    "test:security-advanced": "jest src/tests/security.advanced.test.ts",
    "test:mdx-advanced": "jest src/tests/mdx.advanced.test.ts",
    "test:stress-performance": "jest src/tests/stress.performance.test.ts",
    "test:services": "jest src/tests/*Service.test.ts",
    "test:advanced": "jest src/tests/*.advanced.test.ts",
    "test:all": "node run-all-tests.js",
    "test:all-bash": "./run-all-tests.sh",
    "test:full-suite": "npm run test:comprehensive && npm run test:security-advanced && npm run test:mdx-advanced && npm run test:stress-performance",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build",
    "clean": "rm -rf lib coverage",
    "dev": "tsc --watch"
  },
  "keywords": [
    "TM1",
    "IBM Cognos TM1",
    "Planning Analytics",
    "PA",
    "Cognos",
    "REST API"
  ],
  "author": "KimKaoPoo",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "luxon": "^3.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KimKaoPoo/tm1npm.git"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
